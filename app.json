{
  "expo": {
    "name": "Travlounge",
    "slug": "travlounge",
    "version": "4.1.45",
    "runtimeVersion": "4.1.45",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "ios": {
      "buildNumber": "4001045",
      "config": {
        "googleMapsApiKey": "AIzaSyAfaCR43T4eEiSNhrAs6XgeeYkM49ZwhlA"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.travlounge.app",
      "googleServicesFile": "./google-services.plist",
      "appleTeamId": "PKN3Y7KT68",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationAlwaysUsageDescription": "Allow $(PRODUCT_NAME) to access your location to find nearby services and attractions during your travels.",
        "NSLocationWhenInUseUsageDescription": "Allow $(PRODUCT_NAME) to access your location to find nearby services and attractions during your travels.",
        "LSApplicationQueriesSchemes": ["tez", "phonepe", "paytmmp", "itms-apps"]
      }
    },
    "android": {
      "versionCode": 4001045,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAfaCR43T4eEiSNhrAs6XgeeYkM49ZwhlA"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.travcustomer",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "kotlinVersion": "1.9.25"
          },
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-video",
        {
          "isPictureInPictureSupported": false,
          "supportsBackgroundPlayback": false
        }
      ],
      [
        "expo-notifications",
        {
          "enableBackgroundRemoteNotifications": true
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to access your location to find nearby services and attractions during your travels.",
          "locationAlwaysPermission": "Allow $(PRODUCT_NAME) to access your location to find nearby services and attractions during your travels.",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to access your location to find nearby services and attractions during your travels."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ],
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2fa23468-ff19-438b-ba3d-e3755d4651a1"
      }
    },
    "owner": "beacon-infotech",
    "updates": {
      "url": "https://u.expo.dev/2fa23468-ff19-438b-ba3d-e3755d4651a1"
    }
  }
}
